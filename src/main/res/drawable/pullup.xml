<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="@color/backgroundPullup" />
            <corners android:topLeftRadius="@dimen/pullup_radius" android:topRightRadius="20dp" />
        </shape>
    </item>
    <item android:gravity="center_horizontal|top" android:top="8dp">
        <shape>
            <solid android:color="@color/foreground" />
            <corners android:radius="20dp" />
            <size android:width="50dp" android:height="6dp" />
        </shape>
    </item>
</layer-list>