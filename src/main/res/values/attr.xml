<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LabeledDiscreteSeekBar" >
        <attr name="thumbTextSize" format="dimension" />
        <attr name="thumbPadding" format="dimension" />
        <attr name="thumbPaddingLeft" format="dimension" />
        <attr name="thumbPaddingTop" format="dimension" />
        <attr name="thumbPaddingRight" format="dimension" />
        <attr name="thumbPaddingBottom" format="dimension" />
        <attr name="thumbRadius" format="dimension" />
        <attr name="thumbTint" format="color" />
        <attr name="trackTint" format="color" />
        <attr name="trackHeight" format="dimension" />
        <attr name="labelColor" format="color" />
    </declare-styleable>

    <declare-styleable name="PullupScrollView">
        <attr name="minimumVisibleHeight" format="dimension" />
        <attr name="additionalPaddingBottom" format="dimension" />
        <attr name="backgroundPullup" format="reference" />
        <attr name="backgroundColorPullup" format="color" />
        <attr name="fadeDuration" format="integer" />
        <attr name="fadeInDuration" format="integer" />
        <attr name="fadeOutDuration" format="integer" />
        <attr name="fadeDurationRelative" format="float" />
        <attr name="fadeInDurationRelative" format="float" />
        <attr name="fadeOutDurationRelative" format="float" />
    </declare-styleable>


    <attr name="shadowSize" format="dimension" />
    <attr name="shadowColors" format="reference" />
    <attr name="shadowStops" format="reference" />
    <attr name="radiusInner" format="dimension" />
    <attr name="shadowBelow" format="reference" />
    <attr name="shadowAboveIndex" format="integer" />

    <declare-styleable name="DropShadow">
        <attr name="shadowSize" />
        <attr name="shadowColors" />
        <attr name="shadowStops" />
        <attr name="radiusInner" />
    </declare-styleable>

    <declare-styleable name="DropShadowView">
        <attr name="shadowSize" />
        <attr name="shadowColors" />
        <attr name="shadowStops" />
        <attr name="radiusInner" />
        <attr name="shadowBelow" />
        <attr name="shadowAboveIndex" />
    </declare-styleable>

    <declare-styleable name="FloatingButton">
        <attr name="offsetIconY" format="float" />
    </declare-styleable>

    <declare-styleable name="FloatingInfoButton">
        <attr name="text" format="string" />
        <attr name="icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="MenuTabLayout">
        <attr name="paddingContent" format="dimension" />
    </declare-styleable>

    <declare-styleable name="FlowLayout">
        <attr name="layout_spacing_horizontal" format="dimension" />
        <attr name="layout_spacing_vertical" format="dimension" />
    </declare-styleable>

    <declare-styleable name="InstantAutoCompleteTextView">
        <attr name="textSizeFocus" format="dimension" />
        <attr name="heightFocus" format="dimension" />
    </declare-styleable>

    <declare-styleable name="StationSearchbar">
        <attr name="animated" format="boolean" />
        <attr name="textInfo" format="string" />
        <attr name="fillUntil" format="reference" />
    </declare-styleable>
</resources>